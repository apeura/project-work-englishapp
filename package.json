{
  "name": "fullstack-cp",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "install-all": "npm install",
    "start": "concurrently \"npm run start-frontend\" \"npm run start-backend\"",
    "start-frontend": "npm run dev --workspaces=frontend",
    "start-backend": "npm start --workspaces=backend"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "mysql": "^2.18.1"
  }
}
